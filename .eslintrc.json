{
  "extends": [
    "@movable/eslint-config",
    "@movable/eslint-config/node"
  ],
  "overrides": [
    {
      "files": ["__mocks__/**/*.js", "**/*.spec.js"],
      "plugins": ["jest"],
      "env": {
        "jest/globals": true
      },
      "rules": {
        "node/no-unpublished-require": "off"
      }
    }
  ]
}
